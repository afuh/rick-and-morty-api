    "express-brute-mongoose": "^1.0.2",
    "express-validator": "^4.3.0",
    "graphql": "^0.13.2",
    "graphql-depth-limit": "^1.1.0",
    "graphql-tools": "^2.23.1",
    "moment": "^2.20.1",
    "mongoose": "^5.0.4",
    "mongoose-mongodb-errors": "0.0.2",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
